{
  "name": "my-turbo-nextjs",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "clean": "rimraf node_modules */**/node_modules .turbo */**/.next"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "turbo": "^2.5.2",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
